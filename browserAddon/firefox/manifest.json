{

	"manifest_version": 2,
	"name": "Cyberpanel Download Notifier",
	"version": "1.0",

	"description": "Notify CyberPanel service about download states",
	
	
  "browser_specific_settings": {
  "gecko": {
    "id": "borderify@example.com"
  }
},

	"permissions": [
		"downloads",
		"downloads.open"
	],
	"background": {
		"scripts": ["downloadNotify.js"]
	}
	
}
